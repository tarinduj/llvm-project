// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance2991
func @performance2991() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4, d5, d6, d7)[] : (d7 + -4 = 0 and -d2 + d6 + 1 = 0 and -d1 + d5  = 0 and -d0 + d4  = 0 and d3 + -4 = 0 and d0  >= 0 and -d0 + 511 >= 0 and d1  >= 0 and -d1 + 511 >= 0 and d2 + -1 >= 0 and -d2 + 511 >= 0  or d7 + -2 = 0 and -d2 + d6 + 1 = 0 and -d1 + d5  = 0 and -d0 + d4  = 0 and d3  = 0 and d0  >= 0 and -d0 + 511 >= 0 and d1  >= 0 and -d1 + 511 >= 0 and d2 + -1 >= 0 and -d2 + 511 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<8,0>
  %e = presburger.equal %set, %r : !presburger.set<8,0>, !presburger.set<8,0>
  return %e : i1
}

