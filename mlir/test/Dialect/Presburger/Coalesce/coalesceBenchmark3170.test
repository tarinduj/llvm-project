// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance3170
func @performance3170() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1)[] : (d1 + -2 = 0 and d0  >= 0 and -d0 + 1999 >= 0  or d1 + -1 = 0 and d0  >= 0 and -d0 + 1999 >= 0  or d1  = 0 and d0  >= 0 and -d0 + 1999 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<2,0>
  %e = presburger.equal %set, %r : !presburger.set<2,0>, !presburger.set<2,0>
  return %e : i1
}

