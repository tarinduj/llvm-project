// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance3701
func @performance3701() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4, d5, d6)[] : (d6  = 0 and -d0 + d5  = 0 and -d1 + d4  = 0 and -d0 + d3  = 0 and d2  + -1 = 0 and d0  >= 0 and -d0 + d1  + -2 >= 0 and -d1  + 1999 >= 0  or d6  + -1 = 0 and -d0 + d5  = 0 and -d0 + d4  + -2 = 0 and -d0 + d3  = 0 and d2  + -2 = 0 and -d0 + d1  + -2 = 0 and d0  >= 0 and -d0  + 1997 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<7,0>
  %e = presburger.equal %set, %r : !presburger.set<7,0>, !presburger.set<7,0>
  return %e : i1
}

