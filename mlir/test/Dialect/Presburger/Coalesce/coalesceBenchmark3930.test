// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance3930
func @performance3930() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4)[] : (d4 + -1999 = 0 and -d1 + d3  = 0 and d2 + -2 = 0 and d0  >= 0 and -d0 + 49 >= 0 and d1  >= 0 and -d1 + 1999 >= 0  or d4 + -1999 = 0 and -d1 + d3  = 0 and d2  = 0 and d0  >= 0 and -d0 + 49 >= 0 and d1  >= 0 and -d1 + 1999 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<5,0>
  %e = presburger.equal %set, %r : !presburger.set<5,0>, !presburger.set<5,0>
  return %e : i1
}

