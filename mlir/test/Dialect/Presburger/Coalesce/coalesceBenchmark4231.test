// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance4231
func @performance4231() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4, d5)[] : (d5  = 0 and -d0  >= 0  or d5  = 0 and d0  + -1 = 0 and -d1  + -2 >= 0  or -d1 + d5  + -1 = 0 and d0  + -1 = 0 and d1  >= 0 and -d1  + 1198 >= 0 and d2  + -3 >= 0  or -d1 + d5  = 0 and d0  + -1 = 0 and d1  >= 0 and -d1  + 1199 >= 0 and -d2  + 1 >= 0  or -d1 + d5  + -1 = 0 and d2  + -2 = 0 and d0  + -1 = 0 and d1  >= 0 and -d1  + 1198 >= 0 and d3  + -1400 >= 0  or -d1 + d5  = 0 and d2  + -2 = 0 and d0  + -1 = 0 and d1  >= 0 and -d1  + 1199 >= 0 and -d3  + -2 >= 0  or -d1 + d5  = 0 and d2  + -2 = 0 and d0  + -1 = 0 and d1  >= 0 and -d1  + 1199 >= 0 and d3  >= 0 and -d3  + 1398 >= 0 and d4  + -1 >= 0  or -d1 + d5  = 0 and d2  + -2 = 0 and d0  + -1 = 0 and d1  >= 0 and -d1  + 1199 >= 0 and d3  >= 0 and -d3  + 1399 >= 0 and -d4  >= 0  or -d1 + d5  = 0 and d3  + 1 = 0 and d2  + -2 = 0 and d0  + -1 = 0 and d1  >= 0 and -d1  + 1199 >= 0  or -d1 + d5  + -1 = 0 and d3  + -1399 = 0 and d2  + -2 = 0 and d0  + -1 = 0 and d1  >= 0 and -d1  + 1198 >= 0 and d4  + -1 >= 0  or d5  = 0 and d1  + 1 = 0 and d0  + -1 = 0 )">

  %r = presburger.coalesce %set : !presburger.set<6,0>
  %e = presburger.equal %set, %r : !presburger.set<6,0>, !presburger.set<6,0>
  return %e : i1
}

