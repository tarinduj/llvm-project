// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance4345
func @performance4345() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4)[] : (d3  + -2 = 0 and -d0 + d2  = 0 and d1  + -2 = 0 and d0  >= 0 and -d0  + 1199 >= 0 and -d4  >= 0  or d3  + -1 = 0 and -d0 + d2  = 0 and d1  + -2 = 0 and d0  >= 0 and -d0  + 1199 >= 0 and d4  + -1 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<5,0>
  %e = presburger.equal %set, %r : !presburger.set<5,0>, !presburger.set<5,0>
  return %e : i1
}

