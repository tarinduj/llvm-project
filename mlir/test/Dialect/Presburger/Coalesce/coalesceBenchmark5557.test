// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance5557
func @performance5557() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4)[p0] : (d3 + -2 = 0 and -d0 + d2  = 0 and d1  = 0 and d0 + -1 >= 0 and p0 -d0 + -1 >= 0  or d3 + -3 = 0 and -d0 + d2  = 0 and d1  = 0 and d0 + -1 >= 0 and p0 -d0 + -1 >= 0 and -d4  >= 0  or d3 + -1 = 0 and -d0 + d2  = 0 and d1  = 0 and d0 + -1 >= 0 and p0 -d0 + -1 >= 0 and -d0 + d4  >= 0  or d3 + -3 = 0 and d2  = 0 and d1  = 0 and d0  = 0 and p0 -1 >= 0 and -d4  >= 0  or d3 + -2 = 0 and d2  = 0 and d1  = 0 and d0  = 0 and p0 -1 >= 0 and d4 + -1 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<5,1>
  %e = presburger.equal %set, %r : !presburger.set<5,1>, !presburger.set<5,1>
  return %e : i1
}

