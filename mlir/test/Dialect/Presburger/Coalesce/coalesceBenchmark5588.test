// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance5588
func @performance5588() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0)[] : (d0 + -8 >= 0 and -d0 + 15999 >= 0  or d0  >= 0 and -d0 + 7 >= 0  or d0  >= 0 and -d0 + 15991 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<1,0>
  %e = presburger.equal %set, %r : !presburger.set<1,0>, !presburger.set<1,0>
  return %e : i1
}

