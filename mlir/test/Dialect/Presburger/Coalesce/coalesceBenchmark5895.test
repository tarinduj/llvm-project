// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance5895
func @performance5895() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1)[] : (d0  >= 0 and -d0  + 1099 >= 0 and d1  + -900 >= 0  or d0  >= 0 and -d0  + 1099 >= 0 and -d1  + 898 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<2,0>
  %e = presburger.equal %set, %r : !presburger.set<2,0>, !presburger.set<2,0>
  return %e : i1
}

