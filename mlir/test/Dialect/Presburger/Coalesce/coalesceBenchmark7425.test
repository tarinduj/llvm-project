// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance7425
func @performance7425() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4, d5, d6, d7, d8)[] : (-d3 + 255d4 + -255 >= 0  or d4 + -1 = 0 and d3 + -1 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<9,0>
  %e = presburger.equal %set, %r : !presburger.set<9,0>, !presburger.set<9,0>
  return %e : i1
}

