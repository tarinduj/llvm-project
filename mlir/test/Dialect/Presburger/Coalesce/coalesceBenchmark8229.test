// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8229
func @performance8229() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4)[] : (d3  + -4 = 0 and d1  + -4 = 0 and d0  + -8 >= 0 and d2  >= 0 and -d2  + 1899 >= 0 and -d4  + 2098 >= 0 and -d0 + 8d4  + 15 >= 0 and d0 + -8d4  + -8 >= 0  or d3  + -4 = 0 and d1  + -4 = 0 and -d0  + 16799 >= 0 and d2  >= 0 and -d2  + 1899 >= 0 and d4  >= 0 and -d0 + 8d4  + 15 >= 0 and d0 + -8d4  + -8 >= 0  or d4  = 0 and d3  + -3 = 0 and d1  + -4 = 0 and d0  >= 0 and -d0  + 7 >= 0 and d2  >= 0 and -d2  + 1899 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<5,0>
  %e = presburger.equal %set, %r : !presburger.set<5,0>, !presburger.set<5,0>
  return %e : i1
}

