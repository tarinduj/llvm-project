// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8249
func @performance8249() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0)[p0, p1, p2] : (p + -1 >= 0 and p0 -d0  + -2 >= 0  or p + -1 >= 0 and -p0 d0  >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<1,3>
  %e = presburger.equal %set, %r : !presburger.set<1,3>, !presburger.set<1,3>
  return %e : i1
}

