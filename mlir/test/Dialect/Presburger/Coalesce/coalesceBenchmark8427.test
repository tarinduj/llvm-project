// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8427
func @performance8427() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4)[] : (d0 + -d2 + d3  + -2501 = 0 and -d0  + 2499 >= 0 and d0 + -d1  + -1 >= 0 and d2  >= 0 and d1 + -d2  + -1 >= 0 and d0 + -d1 + d4  + -2500 >= 0 and -d0 + d1 + -d4  + 2503 >= 0  or d0 + d3  + -2499 = 0 and -d0  + 2499 >= 0 and d0 + -d1  + -1 >= 0 and d2  >= 0 and d1 + -d2  + -1 >= 0 and d0 + -d1 + d4  + -2500 >= 0 and -d0 + d1 + -d4  + 2503 >= 0  or d0 + d3  + -2499 = 0 and -d0  + 2499 >= 0 and d0 + -d1  + -1 >= 0 and d2  >= 0 and d1 + -d2  + -1 >= 0 and d0 + -d2 + d4  + -2500 >= 0 and -d0 + d2 + -d4  + 2503 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<5,0>
  %e = presburger.equal %set, %r : !presburger.set<5,0>, !presburger.set<5,0>
  return %e : i1
}

