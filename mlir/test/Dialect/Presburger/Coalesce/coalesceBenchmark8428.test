// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8428
func @performance8428() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2)[] : (d0 + -d1 + d2  + -2500 = 0 and -d0  + 2499 >= 0 and d1  + -1 >= 0 and d0 + -d1  + -1 >= 0  or d0 + d2  + -2499 = 0 and -d0  + 2499 >= 0 and d1  + -1 >= 0 and d0 + -d1  + -1 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<3,0>
  %e = presburger.equal %set, %r : !presburger.set<3,0>, !presburger.set<3,0>
  return %e : i1
}

