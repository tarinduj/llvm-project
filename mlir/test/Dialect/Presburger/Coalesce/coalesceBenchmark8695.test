// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8695
func @performance8695() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4, d5)[] : (-d1 + d4  = 0 and -d0 + d3  = 0 and d0  >= 0 and -d0 + 499 >= 0 and d1  >= 0 and -d1 + 998 >= 0 and d2  >= 0 and -d2 + d5  >= 0 and -d5 + 1198 >= 0 and d2 + -d5 + 1 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<6,0>
  %e = presburger.equal %set, %r : !presburger.set<6,0>, !presburger.set<6,0>
  return %e : i1
}

