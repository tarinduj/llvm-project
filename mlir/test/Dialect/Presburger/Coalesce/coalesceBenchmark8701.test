// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8701
func @performance8701() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4)[] : (d4  + -1199 = 0 and d1  + -1199 = 0 and d2  >= 0 and -d2  + 499 >= 0 and -d0 + d3  + 1 >= 0 and d3  >= 0 and -d3  + 998 >= 0 and d0 + -d3  >= 0  or -d1 + d4  = 0 and d2  = 0 and -d0  + 998 >= 0 and d1  >= 0 and -d1  + 1198 >= 0 and -d0 + d3  + 1 >= 0 and d3  >= 0 and d0 + -d3  >= 0  or -d1 + d4  = 0 and d3  + -998 = 0 and d0  + -999 = 0 and d1  >= 0 and -d1  + 1198 >= 0 and d2  >= 0 and -d2  + 499 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<5,0>
  %e = presburger.equal %set, %r : !presburger.set<5,0>, !presburger.set<5,0>
  return %e : i1
}

