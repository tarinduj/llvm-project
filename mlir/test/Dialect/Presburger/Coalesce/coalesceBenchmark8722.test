// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8722
func @performance8722() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2)[] : (d0  >= 0 and -d0 + 499 >= 0 and d1  >= 0 and -d1 + 997 >= 0 and d2 + -998 >= 0  or d0  >= 0 and -d0 + 499 >= 0 and d1  >= 0 and -d1 + 997 >= 0 and -d2 + 996 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<3,0>
  %e = presburger.equal %set, %r : !presburger.set<3,0>, !presburger.set<3,0>
  return %e : i1
}

