// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8840
func @performance8840() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4, d5)[] : (d5  + -1999 = 0 and d0  + -1999 = 0 and d1  + -1 >= 0  or d5  = 0 and d0  = 0 and d1  + -1 >= 0  or -d0 + d5  = 0 and d1  + -1 = 0 and d0  + -1 >= 0 and -d0  + 1998 >= 0 and -d2  + -1 >= 0  or -d0 + d5  = 0 and d1  = 0 and d0  >= 0 and -d0  + 1999 >= 0 and -d0 + d2  + -1 >= 0  or -d0 + d5  = 0 and -d0 + d2  = 0 and d1  = 0 and d0  >= 0 and -d0  + 1999 >= 0 and d3  + -1 >= 0  or -d0 + d5  = 0 and d2  = 0 and d1  + -1 = 0 and d0  + -1 >= 0 and -d0  + 1998 >= 0 and -d3  + -1 >= 0  or -d0 + d5  = 0 and d3  = 0 and -d0 + d2  = 0 and d1  = 0 and d0  >= 0 and -d0  + 1999 >= 0 and d4  + -1 >= 0  or -d0 + d5  = 0 and d3  = 0 and d2  = 0 and d1  + -1 = 0 and d0  + -1 >= 0 and -d0  + 1998 >= 0 and d0 + -d4  + -1 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<6,0>
  %e = presburger.equal %set, %r : !presburger.set<6,0>, !presburger.set<6,0>
  return %e : i1
}

