// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8859
func @performance8859() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4, d5, d6)[] : (-d0 + d6  + 1 = 0 and -d2 + d5  = 0 and -d0 + d4  + 1 = 0 and d3  + -1 = 0 and d1  + -1 = 0 and d0  + -1 >= 0 and -d0  + 1998 >= 0 and d2  >= 0 and -d2  + 499 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<7,0>
  %e = presburger.equal %set, %r : !presburger.set<7,0>, !presburger.set<7,0>
  return %e : i1
}

