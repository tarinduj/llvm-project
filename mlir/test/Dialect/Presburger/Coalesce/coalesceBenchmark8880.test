// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8880
func @performance8880() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0)[] : (-d0 + 498 >= 0  or d0 + -500 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<1,0>
  %e = presburger.equal %set, %r : !presburger.set<1,0>, !presburger.set<1,0>
  return %e : i1
}

