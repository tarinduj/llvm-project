// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8885
func @performance8885() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2)[] : (d0  >= 0 and -d0  + 499 >= 0 and d1  >= 0 and -d1  + 1997 >= 0 and d2  + -1998 >= 0  or d0  >= 0 and -d0  + 499 >= 0 and d1  >= 0 and -d1  + 1997 >= 0 and -d2  + 1996 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<3,0>
  %e = presburger.equal %set, %r : !presburger.set<3,0>, !presburger.set<3,0>
  return %e : i1
}

